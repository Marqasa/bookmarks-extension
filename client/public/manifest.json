{
  "manifest_version": 3,
  "name": "AI Bookmark Manager",
  "version": "1.0.0",
  "description": "A Chrome extension to manage bookmarks with AI assistance.",
  "icons": {
    "48": "icons/bookmarks.png",
    "96": "icons/bookmarks@2x.png"
  },
  "action": {
    "default_icon": "icons/star-empty-38.png",
    "default_title": "AI Bookmark Manager",
    "default_popup": "index.html"
  },
  "permissions": ["bookmarks", "tabs", "notifications"],
  "background": {
    "type": "module",
    "service_worker": "background.js"
  }
}
